{
  "command": "openscad",
  "maxWorkers": 30,
  "sets": {
    "length_units": [1, 2, 3, 4, 5, 6],
    "width_units": [1, 2, 3, 4, 5, 6],
    "height": ["S", "M", "L"]
  },
  "tasks": {
    "store-hero-box": {
      "title": "Store Hero Box @length_units@x@width_units@@height@",
      "run": ["length_units", "width_units", "height"],
      "commandArgs": [
        "-o", "stl/Store Hero Box/@height@/Store Hero Box @length_units@x@width_units@@height@.stl",
        "-D", "LENGTH_UNITS=@length_units@",
        "-D", "WIDTH_UNITS=@width_units@",
        "-D", "HEIGHT=\"@height@\"",
        "scad/Store Hero Box.scad"
      ],
      "createDirs": ["stl/Store Hero Box/@height@"]
    },
    "store-hero-grid": {
      "title": "Store Hero Grid @length_units@x@width_units@",
      "run": ["length_units", "width_units"],
      "commandArgs": [
        "-o", "stl/Store Hero Grid/Store Hero Grid @length_units@x@width_units@@height@.stl",
        "-D", "LENGTH_UNITS=@length_units@",
        "-D", "WIDTH_UNITS=@width_units@",
        "scad/Store Hero Grid.scad"
      ],
      "createDirs": ["stl/Store Hero Grid"]
    }
  }
}